<petrinet name="pepova sit" author="pepa" version="1" description="prvni pokusna pepikova sit">

  <place id="1">
    <value>3</value>
    <value>8</value>
    <value>10</value>
  </place>

  <place id="2">
    <value>5</value>
  </place>

  <place id="3">
    <value>2</value>
    <value>1</value>
  </place>

  <place id="4">
  </place>

  <transition expr="a = x + y + z">
    <guard src="x" op="&lt;" dst="y" />
    <guard src="y" op="==" dst="x" />
    <guard src="z" op="!=" dst="4" />
    <guard src="x" op="&gt;=" dst="3" />


    <arc io="in" name="x" place="1" />
    <arc io="in" name="4" place="2" />
    <arc io="out" name="y" place="1" />
    <arc io="out" name="a" place="2" />
    <arc io="out" name="6" place="1" />

  </transition>

</petrinet>
